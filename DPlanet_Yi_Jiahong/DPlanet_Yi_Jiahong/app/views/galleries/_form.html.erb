<%= form_for @gallery do |f| %>
  <%= f.error_messages %>
    <p style="display:none">
      <%= f.label :owner %><br />
      <%= f.text_field :owner, :value =>session[:userName],readonly:true %>
    </p>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>


    <p style="display:none">
      <%= f.label :comment %><br />
      <%= f.text_field :comment %>
    </p>
  <p><%= f.submit %></p>
<% end %>
